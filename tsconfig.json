{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/pages/index.jsx", "src/pages/_app.jsx", "src/pages/Jewelery/index.jsx", "src/pages/Electronics/index.jsx", "src/pages/Clothing/index.jsx", "src/pages/Clothing/Woman/index.jsx", "src/pages/Clothing/Men/index.jsx", "src/components/products.jsx", "src/components/product.jsx", "src/components/Products/Products.jsx", "src/components/Products/Product.jsx", "src/components/layout/Layout.jsx", "src/components/Header/Header.jsx", "src/components/Header/Categories/Categories.jsx", "src/components/Body/Banner/Banner.jsx", "src/pages/category/clothing/index.tsx", "src/pages/product/[slug].tsx"],
  "exclude": ["node_modules"]
}
